{
    "name": "elnur/blowfish-password-encoder-bundle",
    "type": "symfony-bundle",
    "description": "Blowfish (bcrypt) based password encoder for Symfony2",
    "keywords": ["security", "blowfish", "bcrypt", "password-encoder"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elnur Abdurrakhimov",
            "email": "elnur@elnur.pro",
            "homepage": "http://www.elnur.pro"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "symfony/security-bundle": "~2.0",
        "ircmaxell/password-compat": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "Elnur\\BlowfishPasswordEncoderBundle": "" }
    },
    "target-dir": "Elnur/BlowfishPasswordEncoderBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
